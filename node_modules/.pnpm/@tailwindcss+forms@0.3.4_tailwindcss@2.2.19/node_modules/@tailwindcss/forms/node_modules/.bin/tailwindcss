#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/abdi_pie/Desktop/vue-gallery/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/abdi_pie/Desktop/vue-gallery/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tailwindcss@2.2.19_ugi4xkrfysqkt4c4y6hkyfj344/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../tailwindcss@2.2.19_ugi4xkrfysqkt4c4y6hkyfj344/node_modules/tailwindcss/lib/cli.js" "$@"
fi
